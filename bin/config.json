{
  "inbounds": {
    "http_inbound": {
      "protocol": "http",
      "setting": {
        "address": "0.0.0.0",
        "port": 10809
      },
      "inStream": "tcp",
      "route": "proxy"
    },
  },
  "outbounds": {
    "trojan": {
      "protocol": "trojan",
      "setting": {
        "address": "us.uif02.xyz",
        "port": 443,
        "password": "abc"
      },
      "outStream": "tls_ws"
    },
    "trojan_mux": {
      "protocol": "trojan",
      "setting": {
        "address": "us.uif02.xyz",
        "port": 443,
        "password": "abc"
      },
      "outStream": "ws_mux"
    },
  },
  "routes": {
    "proxy": {
      "rules": [{
        "chinaOnly": true,
        "outbound": ["freedom"]
      }, {
        "outbound": ["trojan"]
      }]
    },
    "freedom": {
      "rules": [{
        "outbound": ["freedom"]
      }]
    }
  },
  "outStream": {
    "tls_ws": {
      "protocol": "ws",
      "tls": {
        "enabled": true
      },
      "setting": {
        "path": "my_proxy"
      }
    },
    "ws_mux": {
      "protocol": "ws",
      "tls": {
        "enabled": true
      },
      "setting": {
        "path": "my_proxy_mux"
      },
      "mux": {
        "enabled": true,
        "password": "123"
      },
    }
  }
}
