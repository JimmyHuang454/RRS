{
  "config": {
    "log": {
      "level": "debug"
    }
  },
  "outStream": {
    "tcp": {
      "protocol": "tcp"
    }
  },
  "inStream": {
    "tcp": {
      "protocol": "tcp"
    }
  },
  "inbounds": {
    "HTTPIn1": {
      "protocol": "http",
      "setting": {
        "address": "127.0.0.1",
        "port": 0
      },
      "route": "freedom"
    },
    "HTTPIn2": {
      "protocol": "http",
      "setting": {
        "address": "127.0.0.1",
        "port": 0
      },
      "route": "httpIn_to_httpOut"
    }
  },
  "outbounds": {
    "HTTPOut": {
      "protocol": "http",
      "setting": { // same as HTTPIn1
        "address": "127.0.0.1",
        "port": 0
      }
    },
    "block": {
      "protocol": "block"
    }
  },
  "routes": {
    "freedom": {
      "rules": [{
        "outbound": "freedom"
      }]
    },
    "httpIn_to_httpOut": {
      "rules": [{
        "outbound": "HTTPOut"
      }]
    },
    "block": {
      "rules": [{
        "outbound": "block"
      }]
    }
  }
}
