{
  "outbounds": {
    "endPoint": {
      "protocol": "freedom",
      "setting": {
        "strategy": "default"
      },
      "outStream": "tcp_instream"
    },
    "trojan_outbound": {
      "protocol": "trojan",
      "setting": {
        "address": "uif02.xyz",
        "port": 443,
        "password": "password2"
      },
      "outStream": "ws_with_tls"
    },
    "trojan_outbound_mux": {
      "protocol": "trojan",
      "setting": {
        "address": "127.0.0.1",
        "port": 8848,
        "password": "password"
      },
      "outStream": "tcp_mux_outstream"
    }
  },
  "inbounds": {
    "http_inbound": {
      "protocol": "http",
      "setting": {
        "address": "0.0.0.0",
        "port": 8080
      },
      "inStream": "tcp_instream",
      "route": "proxy"
    },
    "trojan_inbound_mux": {
      "protocol": "trojan",
      "setting": {
        "address": "127.0.0.1",
        "port": 8848,
        "tunnelPort": 1,
        "tunnelAddress": "fake",
        "password": "password",
      },
      "inStream": "tcp_mux_instream",
      "route": "proxy"
    },
    "socks5_inbound": {
      "protocol": "socks5",
      "setting": {
        "address": "127.0.0.1",
        "port": 8081
      },
      "inStream": "tcp_instream",
      "route": "proxy"
    },
    "socks5_inbound_mux": {
      "protocol": "socks5",
      "setting": {
        "address": "127.0.0.1",
        "port": 8083
      },
      "inStream": "tcp_instream",
      "route": "mux_route"
    },
  },
  "outStream": {
    "tcp_with_tls": {
      "protocol": "tcp",
      "setting": {},
      "connectionTimeout": 100,
      "tls": {
        "enabled": true,
      },
    },
    "tcp_instream": {
      "protocol": "tcp"
    },
    "tcp_mux_outstream": {
      "protocol": "tcp",
      "mux": {
        "enabled": false,
        "password": "123"
      }
    },
    "ws_with_tls": {
      "protocol": "ws",
      "tls": {
        "enabled": true
      },
      "setting": {
        "path": "uif_trojan"
      }
    }
  },
  "inStream": {
    "tcp_instream": {
      "protocol": "tcp"
    },
    "tcp_mux_instream": {
      "protocol": "tcp",
      "mux": {
        "enabled": false,
        "password": "123"
      }
    }
  },
  "routes": {
    "allowUser": {
      "rules": [{
        "user": ["1"],
        "outbound": "endPoint"
      }]
    },
    "mux_route": {
      "rules": [{
        "outbound": "trojan_outbound_mux"
      }]
    },
    "proxy": {
      "rules": [{
        "outbound": "endPoint",
        "domain": ["regex:.*.cn"]
      }, {
        "outbound": "trojan_outbound",
        "domain": ["google"]
      }, {
        "outbound": "endPoint",
        "chinaOnly": true
      }, {
        "outbound": ["trojan_outbound"]
      }]
    }
  }
}
