{
  "outbounds": {
    "endPoint": {
      "protocol": "freedom",
      "setting": {
        "strategy": "default"
      },
      "outStream": "defalut_tcp"
    },
    "trojanProxy": {
      "protocol": "trojan",
      "setting": {
        "address": "uif02.xyz",
        "port": 443,
        "password": "password2"
      },
      "outStream": "ws_with_tls"
    },
    "localTrojan": {
      "protocol": "trojan",
      "setting": {
        "address": "127.0.0.1",
        "port": 8848,
        "password": "password"
      },
      "outStream": "defalut_tcp"
    }
  },
  "inbounds": {
    "http_inbound": {
      "protocol": "http",
      "setting": {
        "address": "127.0.0.1",
        "port": 8080
      },
      "inStream": "defalut_tcp",
      "route": "allowUser"
    },
    "trojan_inbound": {
      "protocol": "http",
      "setting": {
        "address": "127.0.0.1",
        "port": 8082
      },
      "inStream": "defalut_tcp",
      "route": "proxy"
    },
    "socks5_inbound": {
      "protocol": "socks5",
      "setting": {
        "address": "127.0.0.1",
        "port": 8081
      },
      "inStream": "defalut_tcp",
      "route": "proxy"
    }
  },
  "outStream": {
    "tcp_with_tls": {
      "protocol": "tcp",
      "setting": {},
      "connectionTimeout": 100,
      "mux": {
        "enabled": false,
        "concurrency": 10
      },
      "tls": {
        "enabled": true,
        "alpn": ["h2", "http/1.1"],
        "useSystemRoot": true,
        "certificate": ""
      },

      "fakeRoute": {
        "enabled": false
      }
    },
    "defalut_tcp": {
      "protocol": "tcp"
    },
    "ws_with_tls": {
      "protocol": "ws",
      "tls": {
        "enabled": true
      },
      "setting": {
        "path": "uif_trojan"
      }
    }
  },
  "inStream": {
    "defalut_tcp": {
      "protocol": "tcp"
    }
  },
  "route": {
    "allowUser": {
      "rules": [{
        "user": ["1"],
        "outbound": "endPoint"
      }]
    },
    "proxy": {
      "rules": [{
        "user": ["1"],
        "outbound": "trojanProxy"
      }]
    }
  }
}
