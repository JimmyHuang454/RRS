{
  "outbounds": {
    "terminal": {
      "protocol": "freedom",
      "setting": {
        "strategy": "default"
      },
      "outStream": "defalut_tcp"
    },
    "proxy": {
      "protocol": "http",
      "setting": {
        "address": "rr2s.org",
        "port": 443,
        "userAccount": "jimmy",
        "userPassword": "RR2S"
      },
      "outStream": "tcp_with_tls"
    }
  },
  "inbounds": {
    "http_inbound": {
      "protocol": "http",
      "setting": {
        "address": "127.0.0.1",
        "port": 8080
      },
      "inStream": "defalut_tcp",
      "route": "allowUser"
    }
  },
  "outStream": {
    "tcp_with_tls": {
      "protocol": "tcp",
      "setting": {},
      "connectionTimeout": 100,
      "mux": {
        "enabled": false,
        "concurrency": 10
      },
      "tls": {
        "enabled": true,
        "alpn": ["h2", "http/1.1"],
        "useSystemRoot": true,
        "certificate": ""
      },

      "fakeRoute": {
        "enabled": false
      }
    },
    "defalut_tcp": {
      "protocol": "tcp"
    }
  },
  "inStream": {
    "defalut_tcp": {
      "protocol": "tcp"
    }
  },
  "route": {
    "allowUser": {
      "rules": [{
        "user": ["1"],
        "outbound": "terminal"
      }]
    }
  }
}
